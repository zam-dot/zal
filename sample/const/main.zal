@c {
    #include <stdio.h>
    #include <math.h>
    #include <stdlib.h>
}

const S_CONST = "contant"

func main() {
    print("%s\n", S_CONST)

    var n: int = 500000000

    print("%d\n", n)

    var d: double = 3e20 / n  // not parsing after /
    print("%lf\n", d)

    print("%f\n", sin(d))    
}
