@c {
    #include <stdio.h>
}

enum Color {
    Red,
    Green,
    Blue
}

func main() {
    c1 := Color { Red }      // Will print 0
    c2 := Color { Green }    // Will print 1  
    c3 := Color { Blue }     // Will print 2
    
    print("c1 is %d\n", c1)
    print("c2 is %d\n", c2)
    print("c3 is %d\n", c3)
}
